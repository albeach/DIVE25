# Create directory structure script
cat > organize.sh << 'EOF'
#!/bin/bash

echo "🏗️  Organizing DIVE25 repository..."

# Create main directories
mkdir -p {src/{backend/middleware,frontend},docker/{kong,keycloak,postgres},tests}

# Move files to appropriate directories
echo "Moving files to correct locations..."

# Backend files
mv server.js src/backend/ 2>/dev/null
mv middleware/* src/backend/middleware/ 2>/dev/null

# Docker files
mv kong.conf docker/kong/ 2>/dev/null
mv keycloak.json docker/keycloak/ 2>/dev/null

# Test files
mv *.test.js tests/ 2>/dev/null
mv *.spec.js tests/ 2>/dev/null

echo "✨ Organization complete!"
EOF

chmod +x organize.sh
